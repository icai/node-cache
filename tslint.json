{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "eofline": false,
    "quotemark": [
        true,
        "single"
    ],
    "semicolon": [false, "always", "ignore-bound-class-methods"],
    "trailing-comma": [false, {"multiline": "never", "singleline": "never"}],
    "object-literal-sort-keys": false,
    "triple-equals": false
  },
  "rulesDirectory": []
}